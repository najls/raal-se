---
lang: sv
title: Raal - Nyheter
description: Det senaste från Raal rakt in i din mobil eller nåt.
page: /
permalink: /feeds/nyheter.xml
---
<?xml version="1.0" encoding="UTF-8"?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
    <channel>
        <title>{{ page.title | xml_escape }}</title>
        <link>https://www.raal.se/</link>
        <atom:link href="https://www.raal.se{{ page.permalink }}" rel="self" type="application/rss+xml" />
        <description>{{ page.description | xml_escape }}</description>
        <language>{{ page.lang }}</language>
        <lastBuildDate>{{ site.posts.last.date | date_to_rfc822 }}</lastBuildDate>
        {% assign posts = site.posts | where: "lang", page.lang %}
        {% for post in posts %}
            <item>
                <title>{{ post.title | xml_escape }}</title>
                <link>https://www.raal.se{{ page.page }}#{{ post.r_id }}</link>
                <description>{{ post.content | xml_escape }}</description>
                <guid>https://www.raal.se/{{ post.r_id | append: post.date | uri_escape }}</guid>
                <pubDate>{{ post.date | date_to_rfc822 }}</pubDate>
                <source url="https://www.raal.se{{ page.permalink }}">Raal</source>
            </item>
        {% endfor %}
    </channel>
</rss>